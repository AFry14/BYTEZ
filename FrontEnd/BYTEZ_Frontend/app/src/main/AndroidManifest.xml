<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.Bytez_frontend">

    <uses-permission android:name = "android.permission.INTERNET" />
    <application
        android:allowBackup="true"
        android:usesCleartextTraffic="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".login.SignUp"></activity>
        <activity android:name=".login.SuccessActivity"></activity>
        <activity android:name=".login.LoginActivity"></activity>
        <activity android:name=".Map.HomeActivity"></activity>
        <activity android:name=".Map.MapActivity"></activity>
        <activity android:name=".ReviewPackage.ReviewActivity"
            android:exported = "true">
        </activity>

        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>